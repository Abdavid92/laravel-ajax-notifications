{
    "name": "abdavid92/laravel-ajax-notifications",
    "description": "Asynchronous notifications for Laravel without Web Sockets",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Abdavid92\\LaravelAjaxNotifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abdavid92\\LaravelAjaxNotifications\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Abdavid92",
            "email": "lleraabi@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "laravel/framework": "^9.45"
    },
    "require-dev": {
        "orchestra/testbench": "v7.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abdavid92\\LaravelAjaxNotifications\\AjaxNotificationsServiceProvider"
            ]
        }
    }
}
